{
  "version": 2,
  "builds": [
    {
      "src": "./public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "./index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
        "src": "/",
        "dest": "/public/index.html"
    },
    {
        "src": "/(.*)",
        "dest": "/"
    }
  ]
}
